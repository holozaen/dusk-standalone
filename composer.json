{
    "name": "konsulting/dusk-standalone",
    "description": "Standalone dusk testing, ideal for legacy projects, or remote testing.",
    "require": {
        "php": ">=7.1",
        "phpunit/phpunit": "~7.0",
        "laravel/dusk": "^4.0",
        "facebook/webdriver": "^1.3",
        "konsulting/project-root": "^1.1",
        "orchestra/dusk-updater": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Keoghan Litchfield",
            "email": "keoghan@klever.co.uk"
        }
    ],
    "autoload" : {
        "psr-4": {
            "Konsulting\\DuskStandalone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konsulting\\DuskStandalone\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
